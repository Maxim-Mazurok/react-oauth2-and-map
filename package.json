{
  "name": "react-oauth2-and-map",
  "version": "1.0.0",
  "description": "NewMotion Use Case",
  "engines": {
    "node": "10.16.3",
    "npm": "6.10.3"
  },
  "main": "index.js",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "scripts": {
    "start": "npm run start-fake-server & webpack-dev-server --open --mode development --host=0.0.0.0",
    "start-fake-server": "tsc --outDir dist src/fake-server.ts && node dist/fake-server.js",
    "build": "NODE_ENV=production webpack --mode production",
    "test": "jest -c jest.config.js",
    "test-unit": "jest -c jest.config.unit.js",
    "test-integration": "jest -c jest.config.integration.js",
    "update-snapshots": "jest --updateSnapshot",
    "test-watch": "jest --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "tslint-check": "tslint --project tsconfig.json",
    "tslint-fix": "tslint --project tsconfig.json --fix",
    "prettier-check": "prettier {{src,test}/**/*.{tsx,ts,js,json,scss,css,html},*.{js,json,md}} --check",
    "prettier-fix": "prettier {{src,test}/**/*.{tsx,ts,js,json,scss,css,html},*.{js,json,md}} --write",
    "stylelint-check": "stylelint **/*.scss",
    "stylelint-fix": "stylelint **/*.scss --fix",
    "eslint-check": "eslint --max-warnings=0 --ext .ts --ext .tsx src/",
    "eslint-fix": "eslint --ext .ts --ext .tsx src/ --fix",
    "all-checks": "npm run typecheck && npm run tslint-check && npm run prettier-check && npm run stylelint-check && npm run eslint-check",
    "all-fixes": "npm run typecheck && npm run tslint-fix && npm run prettier-fix && npm run stylelint-fix && npm run eslint-fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "PATH=~/.nvm/versions/node/v$(grep '\"node\"' package.json | cut -d '\"' -f 4)/bin:$PATH && npm run all-checks && npm test",
      "pre-push": "PATH=~/.nvm/versions/node/v$(grep '\"node\"' package.json | cut -d '\"' -f 4)/bin:$PATH && npm run all-checks && npm test"
    }
  },
  "keywords": [
    "NewMotion",
    "maps",
    "react",
    "OAuth2"
  ],
  "author": "Maxim Mazurok <maxim@mazurok.com> (https://maxim.mazurok.com)",
  "private": true,
  "license": "Proprietary",
  "dependencies": {
    "@google/markerclustererplus": "^2.1.11",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@testing-library/dom": "^6.1.0",
    "@testing-library/react": "^9.1.3",
    "@types/googlemaps": "^3.37.4",
    "@types/jest": "^24.0.18",
    "@types/markerclustererplus": "^2.1.33",
    "@types/node": "^12.7.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-remove-data-test-id": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "bundle-loader": "^0.5.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-standard-react": "^9.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.3.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.9.4",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "tslint-webpack-plugin": "^2.1.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
