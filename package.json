{
  "name": "react-oauth2-and-map",
  "version": "1.0.0",
  "description": "NewMotion Use Case",
  "engines": {
    "node": "10.16.2",
    "npm": "6.10.3"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"No test specified\"",
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "tslint-check": "tslint --project tsconfig.json",
    "tslint-fix": "tslint --project tsconfig.json --fix",
    "prettier-check": "prettier {{src,test}/**/*.{tsx,ts,js,json,scss,css,html},*.{js,json,.babelrc}} --check",
    "prettier-fix": "prettier {{src,test}/**/*.{tsx,ts,js,json,scss,css,html},*.{js,json,.babelrc}} --write",
    "stylelint-check": "stylelint **/*.scss",
    "stylelint-fix": "stylelint **/*.scss --fix",
    "all-checks": "npm run tslint-check && npm run prettier-check && npm run stylelint-check",
    "all-fixes": "npm run tslint-fix && npm run prettier-fix && npm run stylelint-fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run all-checks && npm test",
      "pre-push": "npm run all-checks && npm test"
    }
  },
  "keywords": [
    "NewMotion",
    "maps",
    "react",
    "OAuth2"
  ],
  "author": "Maxim Mazurok <maxim@mazurok.com> (https://maxim.mazurok.com)",
  "private": true,
  "license": "Proprietary",
  "dependencies": {
    "@types/react": "^16.8.25",
    "@types/react-dom": "^16.8.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.3",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.9.3",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "tslint-webpack-plugin": "^2.1.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
